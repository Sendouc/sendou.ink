    "@types/react-select": "^4.0.13",
    "@types/recharts": "^1.8.19",
    "@types/uuid": "^8.3.0",
    "cross-env": "^7.0.3",
    "cypress": "^6.7.1",
    "prettier": "^2.2.1",
